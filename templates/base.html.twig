      <li class="nav-item">
        <a class="nav-link" href="{{path ('MisPosts')}}">Mis Posts</a>
      </li>
      
      
    </ul>
    <form class="form-inline my-2 my-lg-0">
    <input class="form-control mr-sm-2 barra-navegacion" type="search" placeholder="Busca en Milone" aria-label="Search">
